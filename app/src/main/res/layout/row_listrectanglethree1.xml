<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrectanglethree1RowModel" type="com.tour_guide.app.modules.cssscreen450homeone.data.model.Listrectanglethree1RowModel"/>
    </data>
    <FrameLayout android:id="@+id/frameStackrectanglethree"
        android:layout_height="wrap_content" android:layout_width="@dimen/_185pxh">

        <ImageView
            android:id="@+id/imageRectangleThree"
            cornerRadius="@{@dimen/_27pxh}"
            imageRes="@{@drawable/img_rectangle3}"
            android:layout_width="@dimen/_185pxh"
            android:layout_height="@dimen/_237pxh"
            android:layout_gravity="start"
            android:scaleType="fitXY"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_rectangle3" />

        <LinearLayout android:id="@+id/linearColumnmilanopark"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_1">
            <TextView android:ellipsize="end"
                android:id="@+id/txtMilanoPark"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listrectanglethree1RowModel.txtMilanoPark}"
                style="@style/txtDMSansbold16_1" tools:text="@string/lbl_milano_park"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlocation"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLocationOne"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_11pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_location_10x10"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_location_10x10"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCountryOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listrectanglethree1RowModel.txtCountryOne}"
                    style="@style/txtDMSansmedium11" tools:text="@string/msg_sant_paulo_mil"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowstar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageStar"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_star"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_weight="0.9"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listrectanglethree1RowModel.txtPrice}"
                    style="@style/txtDMSansmedium11" tools:text="@string/msg_4_0_36_review"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
